{
  "name": "new-flow-app",
  "version": "1.1.0",
  "main": "app/index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "build": "yarn webpack",
    "postbuild": "yarn run react-snap",
    "prepublish": "yarn run build",
    "postinstall": "yarn run flow:stub",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "yarn run eslint --config .eslintrc.json --ext .jsx --ext .js app __tests__ && yarn run flow",
    "flow:stub": "yarn run flow-typed install",
    "start:dist": "yarn run simple-server dist 8080"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.70.0",
    "flow-typed": "^2.4.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "react-snap": "^1.12.2",
    "simple-server": "^1.1.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "moduleDirectories": [
      ".",
      "src",
      "node_modules"
    ]
  },
  "reactSnap": {
    "source": "dist"
  }
}
